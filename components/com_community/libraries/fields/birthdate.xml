<?xml version="1.0" encoding="utf-8"?>
<config>
	<params>
		<param name="@spacer" type="spacer" default="" label="" description="" />
		<param name="display" type="radio" default="age" label="Display As" description="">
			<option value="age">Age</option>
			<option value="date">Date</option>
		</param>
		<param name="readonly" type="radio" default="0" label="Read Only" description="">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="maxrange" type="text" default="" label="Maximum Range in YYYY-MM-DD or numeric age" description="" />
		<param name="minrange" type="text" default="" label="Minimum Range in YYYY-MM-DD or numeric age" description="" />
		<param name="date_format" type="text" default="" label="Date Format.(MySQL - E.g. %W, %d %b %Y -> Day, date Month Year)" description="" />
		<param name="style" type="text" default="" label="Style Atrribute" description="" />
	</params>
</config>